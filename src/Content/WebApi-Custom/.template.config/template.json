  "symbols": {
    "Framework": {
      "type": "parameter",
      "description": "The target framework for the project.",
      "datatype": "choice",
      "choices": [
        {
          "choice": "net5.0",
          "description": "Target net5.0"
        }
      ],
      "replaces": "net5.0",
      "defaultValue": "net5.0"
    },
    "excludeSamples": {
      "type": "parameter",
      "datatype": "bool",
      "description": "Create sample code",
      "defaultValue": "false"
    },
    "excludeWarmup": {
      "type": "parameter",
      "datatype": "bool",
      "description": "Exclude warmup capabilities",
      "defaultValue": "false"
    }
  },
  "placeholderFilename": "-.-",
  "sources": [
    {
      "modifiers": [
        {
          "condition": "(excludeSamples)",
          "exclude": [
            "**/*Sample*.cs"
          ]
        },
        {
          "condition": "!(warmup)",
          "exclude": [
            "**/*WarmUp/**"
          ]
        }
      ]
    }
  ],
  "primaryOutputs": [
    {
      "path": "WebApi.sln"
    }
  ]
